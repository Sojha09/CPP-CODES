#include <bits/stdc++.h>
using namespace std;

int main() {
    string s;
    cout << "Enter a string: ";
    getline(cin, s);
string result = "";
    for (char c : s) {
        if (isalpha(c)) {
            result += c;
        }
    }
cout << "String after removing non-alphabet characters: " << result << endl;

    return 0;
}
