#include <bits/stdc++.h>
using namespace std;

int toggleBitsInRange(int n, int L, int R) {
    int mask = ((1 << (R - L + 1)) - 1) << (L - 1);
    return n ^ mask;
}

int main() {
    int n, L, R;
    cout << "Enter number: ";
    cin >> n;
    cout << "Enter range (L R): ";
    cin >> L >> R;

    int result = toggleBitsInRange(n, L, R);
    cout << "Result after toggling bits: " << result << endl;

    return 0;
}
