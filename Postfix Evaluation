class Solution {
  public:
    int evaluatePostfix(vector<string>& arr) {
        stack<int> st;
        for (auto &x : arr) {
            if (x != "+" && x != "-" && x != "*" && x != "/") {
                st.push(stoi(x));
            } else {
                int b = st.top(); st.pop();
                int a = st.top(); st.pop();
                if (x == "+") st.push(a + b);
                else if (x == "-") st.push(a - b);
                else if (x == "*") st.push(a * b);
                else st.push(a / b);
            }
        }
        return st.top();
    }
};
