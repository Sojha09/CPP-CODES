#include <bits/stdc++.h>
using namespace std;

string removeDuplicates(string s) {
    if (s.length() == 0) return "";
    char ch = s[0];
    string ans = removeDuplicates(s.substr(1));
    if (ans.length() > 0 && ans[0] == ch)
        return ans;
    else
        return ch + ans;
}

int main() {
    string s;
    cin >> s;
    cout << removeDuplicates(s);
    return 0;
}
